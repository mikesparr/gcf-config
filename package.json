{
  "name": "gcf-config",
  "version": "1.2.1",
  "description": "Reusable configuration module using ENV vars for cloud function apps",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikesparr/gcf-config.git"
  },
  "keywords": [
    "cloud",
    "functions",
    "12factor",
    "config",
    "environment",
    "variables"
  ],
  "author": "Mike Sparr <mike@goomzee.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikesparr/gcf-config/issues"
  },
  "homepage": "https://github.com/mikesparr/gcf-config#readme",
  "dependencies": {
    "@types/node": "^10.9.4"
  }
}
